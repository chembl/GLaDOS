  describe "Pagination Utilities", ->

    it 'works when less than 5 pages', ->
      [show_previous_ellipsis, show_next_ellipsis, first_page_to_show, last_page_to_show] = \
      glados.Utils.Pagination.calculatePaginatorParams(5, 2)

      expect(show_previous_ellipsis).toBe(false)
